@model List<AppRole>


<a asp-action="Create" asp-controller="Role">Add New Role</a>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Role Name</th>
            <th>Created Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (AppRole item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.CreatedDate</td>
            </tr>
        }
    </tbody>
</table>